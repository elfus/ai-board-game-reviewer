{
  "name": "ai-board-game-reviewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "scrap": "node src/scripts/scrap.js > src/api/gameboard-list.json",
    "comments": "node src/scripts/comments.js > src/api/gameboard-list-comments.json",
    "score": "node src/scripts/score.js > src/api/gameboard-list-scores.json",
    "server": "npx json-server src/api/gameboard-list-scores.json",
    "prepare": "husky",
    "project": "concurrently --kill-others \"npm run server\" \"npm run dev\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.23",
    "ollama-ai-provider": "^0.11.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.51.23",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "ai": "^3.3.0",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.1.4",
    "json-server": "^0.17.4",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.4",
    "vite-plugin-eslint": "^1.8.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  }
}
